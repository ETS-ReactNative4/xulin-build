{
  "presets": [
    "react",
    "es2015"
  ],
  "env": {
    "development": {
      "presets": [
        "react-hmre"
      ]
    }
  },
  "plugins": [
    "syntax-dynamic-import",
    "transform-object-rest-spread",
    "transform-class-properties",
    "transform-decorators-legacy",
    ["babel-plugin-import", {
      "libraryName": "@alifd/next",
      "style": true
    }]
  ]
}
